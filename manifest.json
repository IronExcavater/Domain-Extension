{
  "name": "Extra Domain Filters",
  "version": "0.2",
  "author": "Niclas Rogulski",
  "description": "Enhance domain.com.au with extra filters to empower your search.",
  "homepage_url": "https://github.com/IronExcavater",
  "manifest_version": 3,
  "minimum_chrome_version": "88",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://www.domain.com.au/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.domain.com.au/*"
      ],
      "js": ["domain.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["*://www.domain.com.au/*"],
      "resources": ["domain.js"]
    }
  ]
}
